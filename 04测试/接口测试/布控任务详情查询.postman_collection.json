						"exec": [
							"tests[\"Body matches string error\"] = responseBody.has(\"error\");",
							"",
							"tests[\"Body matches string message\"] = responseBody.has(\"message\");",
							"",
							"tests[\"Status code is 200\"] = responseCode.code === 200;",
							"",
							"tests[\"Response time is less than 200ms\"] = responseTime < 200;",
							"",
							"tests[\"Content-Type is present\"] = postman.getResponseHeader(\"Content-Type\");",
							"",
							"var jsonData = JSON.parse(responseBody.replace(/'/g,'\"'));",
							"console.log(jsonData);",
							"tests[\"error\"] = jsonData[\"error\"] === 0;",
							"tests[\"message\"] = jsonData[\"message\"] === \"successful\";"
						]
